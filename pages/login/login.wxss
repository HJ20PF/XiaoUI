page {
  width: 100%;
  height: 100%;
  background: #fff;
}
.wrap {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
image {
  margin: 100rpx 0 50rpx;
}
.wrap>view:nth-child(2) {
  line-height: 50rpx;
  color: #333;
  font-size: 30rpx;
}
.wrap>view:nth-child(3) {
  line-height: 50rpx;
  color: #999;
  font-size: 26rpx;
}
.wrap>view:nth-child(4) {
  line-height: 50rpx;
  color: #333;
  font-size: 30rpx;
}
.wrap>view:nth-child(3) {
  line-height: 50rpx;
  color: #333;
  font-size: 24rpx;
}
.btn,.no-idont{
  width: 500rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin-top: 40rpx;
  background: #1AAD19;
  color: #fff;
  border-radius: 10rpx;
  font-size: 34rpx;
}
.wrap .btn{
    padding: 0px 24px;
    width: 500rpx;
}
.no-idont{ margin:0 auto; margin-top:30rpx;text-align: center}
.button-hover {
  opacity: 0.8;
}

.formidBtn{
  /* padding:0;background:none; */
  position: absolute;
  top: 0;
  bottom: 0;
  left:0;
  right: 0;
  background: transparent;
  z-index: 999999;
}
.formidBtn::after{
  border: none;
}