/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/*tab*/
.tab {
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  background: #fff;
  border-top: 2rpx solid #acacac;
  height: 92rpx;
  padding-bottom: 4rpx;
  z-index: 99999;
}
.ipx {
  padding-bottom: 68rpx;
}
.tabnav {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.tablist {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: #8c8c8c;
}
.tabimg {
  width: 60rpx;
  height: 60rpx;
  margin-top: 8rpx;
  margin-bottom: 0rpx;
}
.foot {
  height: 100rpx;
}